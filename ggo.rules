<?xml version="1.0" encoding="utf-8"?>
<VisualStudioToolFile
	Name="GetOptionGenerator"
	Version="8.00"
	>
	<Rules>
		<CustomBuildRule
			Name="GetOptionGenerator"
			DisplayName="GetOption Generator"
			CommandLine="gengetopt.exe -G -F [GeneratedName] -i $(InputDir)$(InputName).ggo"
			Outputs="$(InputDir)$(GeneratedName).c;$(InputDir)$(GeneratedName).h"
			FileExtensions="*.ggo"
			ExecutionDescription="Preprocessing: $(InputName).ggo -&gt; $(GeneratedName).[ch] (Include name: [IncludeName])"
			>
			<Properties>
				<StringProperty
					Name="GeneratedName"
					DisplayName="Generated File Name"
					Category="Ggo"
					Description="Ggo"
					Switch="[value]"
				/>
			</Properties>
		</CustomBuildRule>
	</Rules>
</VisualStudioToolFile>
